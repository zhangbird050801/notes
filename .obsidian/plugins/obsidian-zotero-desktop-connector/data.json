{
  "database": "Zotero",
  "noteImportFolder": "Papers/",
  "pdfExportImageDPI": 120,
  "pdfExportImageFormat": "jpg",
  "pdfExportImageQuality": 90,
  "citeFormats": [
    {
      "name": "Cite",
      "format": "formatted-bibliography",
      "cslStyle": "ieee"
    }
  ],
  "exportFormats": [
    {
      "name": "Zotero Note",
      "outputPathTemplate": "Papers/{{title}}.md",
      "imageOutputPathTemplate": "Papers/attachments/",
      "imageBaseNameTemplate": "image",
      "cslStyle": "ieee",
      "templatePath": "@Templates/zotero-note.md"
    }
  ],
  "citeSuggestTemplate": "[[{{citekey}}]]",
  "openNoteAfterImport": true,
  "whichNotesToOpenAfterImport": "all-imported-notes"
}